extends layout

append head
append style
append content

    include navbar
    #staticBackdrop.modal.fade(data-bs-backdrop='static' data-bs-keyboard='false' tabindex='-1' aria-labelledby='staticBackdropLabel' aria-hidden='true')
        .modal-dialog.modal-lg
            .modal-content
                .modal-header
                    h5#staticBackdropLabel.modal-title Service Cart
                    button.btn-close(type='button' data-bs-dismiss='modal' aria-label='Close')
                .modal-body
                    form#bookingForm(action='/action_page.php' method='get' name='myForm')
                        //- .row.justify-content-between.text-left
                        .form-group.col-sm-8.flex-column.d-flex
                            p
                                label.form-label(for='exampleFormControlInput1') First Name
                                input.form-control.form-control-lg#fname(type='text' name='fname' placeholder='Enter your first name' onblur='validate(1)')
                        .form-group.col-sm-8.flex-column.d-flex
                            p
                                label.form-label(for='exampleFormControlInput1') Last Name
                                input.form-control.form-control-lg#lname(type='text' name='lname' placeholder='Enter your last name' onblur='validate(2)')
                            //- .row.justify-content-between.text-left
                        .form-group.col-sm-8.flex-column.d-flex
                            p 
                                label.form-label(for='exampleFormControlInput1') Email address
                                input.form-control.form-control-lg#email(type='text' name='email' placeholder='youremail@what.com' onblur='validate(3)')
                        .form-group.col-sm-8.flex-column.d-flex 
                            p
                                label.form-label(for='exampleFormControlInput1') Phone Number
                                input.form-control.form-control-lg#phone(type='text' name='phone' placeholder='123-456-7890' onblur='validate(4)')
                            //- .row.justify-content-between.text-left
                        .form-group.col-sm-8.flex-column.d-flex
                            p
                                label.form-label(for='exampleFormControlInput1') Location / Address
                                input.form-control.form-control-lg#address(type='text' name='address' placeholder='1234 Relax Ln, Scottsdale, AZ 85200' onblur='validate(5)')
                        .form-group.col-sm-8
                            p
                                button.btn.btn-secondary.m-2(type='button' data-bs-dismiss='modal') Cancel
                                button.btn.btn-primary.m-2(type='submit') Purchase

                //- .modal-footer
                //-     .form-group.col-sm-8
                //-         button.btn.btn-secondary(type='button' data-bs-dismiss='modal') Cancel
                //-         button.btn.btn-primary(type='submit') Purchase

